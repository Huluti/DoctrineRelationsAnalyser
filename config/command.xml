<?xml version="1.0" encoding="UTF-8"?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>

        <service id="doctrinerelationsanalyserbundle.command.analysecommand"
            class="DoctrineRelationsAnalyserBundle\Command\AnalyseCommand" public="false">
            <argument type="service" id="Doctrine\ORM\EntityManagerInterface" />
            <tag name="console.command" command="doctrine-relations-analyser:analyse" />
        </service>

    </services>

</container>
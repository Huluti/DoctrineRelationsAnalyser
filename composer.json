{
    "name": "huluti/doctrine-relations-analyser",
    "description": "A Symfony bundle that provide tools to analyze Doctrine relations",
    "license": "GPL-3.0-or-later",
    "keywords": [
        "bundle",
        "symfony",
        "doctrine",
        "relations"
    ],
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Hugo Posnic",
            "email": "hugo.posnic@protonmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "DoctrineRelationsAnalyserBundle\\": "src/"
        }
    },
    "require": {
        "php": ">=8.0",
        "symfony/console": "^7.1",
        "symfony/http-kernel": "^7.1",
        "doctrine/orm": "^3.0",
        "symfony/dependency-injection": "^7.1"
    },
    "suggest": {
        "graphp/graphviz": "To generate graphviz graphs"
    }
}